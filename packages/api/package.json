{
  "name": "@uma/api",
  "version": "3.0.0",
  "private": true,
  "description": "UMA Ticketing API (Fastify + BullMQ) to open Discord tickets via env-configured bot",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/UMAprotocol/protocol"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "node --loader ts-node/esm --no-warnings=ExperimentalWarning src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "worker": "node --loader ts-node/esm --no-warnings=ExperimentalWarning src/worker.ts",
    "start:worker": "node dist/worker.js",
    "lint": "echo \"No linter configured in this package\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "discord-api-types": "^0.37.83",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/sensible": "^6.0.1",
    "bullmq": "^5.12.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.3.2",
    "pino": "^9.5.0",
    "superstruct": "^1.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}