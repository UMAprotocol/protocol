    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@uma/core": "^2.2.0",
    "ethers": "^5.1.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0"
  }
}
