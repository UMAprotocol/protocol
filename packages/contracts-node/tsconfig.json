{
  "extends": "@tsconfig/node14/tsconfig.json",
  "esModuleInterop": true,
  "include": [
    "generated/index.ts",
    "typechain/**/*",
    "../core/artifacts/**/*.json"
  ],
  "exclude": [
    "../core/artifacts/build-info/**/*.json",
    "../core/artifacts/**/*.dbg.json"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "module": "CommonJS"
  }
}
